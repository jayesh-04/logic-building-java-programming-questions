Find the second largest element

public class Main {
    public static void main(String[] args) {
        int[] A = {10, 20, 5, 8, 30};

        int max = A[0];
        int second = A[0];

        for (int i = 0; i < A.length; i++) {
            if (A[i] > max) {
                second = max;
                max = A[i];
            }
        }

        System.out.println("Second Largest: " + second);
    }
}
