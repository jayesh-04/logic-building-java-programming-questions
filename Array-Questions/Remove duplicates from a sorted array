Remove duplicates from a sorted array.

public class Main {
    public static void main(String[] args) {
        int[] A = {1, 1, 2, 2, 3, 4, 4};
        int j = 0;
        for(int i=1;i<A.length;i++){
            if(A[i]!=A[j]){
                j++;
                A[j]=A[i];
            }
        }
        for(int i=0;i<=j;i++){
            System.out.print(A[i]+" ");
        }
    }
}

