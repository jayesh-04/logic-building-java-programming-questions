Move all zeros to the end

public class Main {
    public static void main(String[] args) {
        int[] A = {1, 1, 0, 2, 0, 0, 4};
        int j = 0;
        for(int i=0;i<A.length;i++){
            if(A[i]!=0){
                A[j]=A[i];
                j++;
            }
        }
        while(j<A.length){
            A[j]=0;
            j++;
        }
        for(int i=0;i<A.length;i++){
            System.out.print(A[i]+" ");
        }
    }
}
